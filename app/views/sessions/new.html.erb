<h1>Login</h1>



<%= form_for :user do |f| %>
  <%= f.text_field :email, placeholder: "Enter your email" %>
  <%= f.password_field :password, placeholder: "Enter your password"%>
  <%= f.submit "Login", class: "btn btn-warning" %>
<% end %>

<%= link_to "Back", root_path, class: "btn btn-success glyphicon glyphicon-chevron-left" %>
