<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>



<div class="container"><%= link_to "New Post", new_path, class: "btn btn-primary glyphicon glypnicon-plus pull-left" %></div>

<div class="row">

<table class="table table-striped">
<% @posts.each do |post| %>
<tr>

<td><li><%= link_to post.title, post.link %></li></td>
</tr>
<% end %>
</table>
</div>

